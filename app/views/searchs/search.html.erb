<div class="container">
    <div class="row no1">
      <div class="col-xs-3">
    
      </div>
      <div class="col-xs-9">
      <% if @model == 'user' %>
      <h3>Users search for '<%= @content %>'</h3>
      <%= render 'user/users/index', users: @detas %>
      <% else @model == 'menu' %>
      <h3>Menus search for '<%= @content %>'</h3>
        <%= render 'user/menus/index', menus: @detas %>
      <% end %>
      </div>
    </div>
</div>