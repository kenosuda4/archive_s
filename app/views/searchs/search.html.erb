<div class="container">
    <div class="row no1">
      <div class="col-xs-3">
    
      </div>
      <div class="col-xs-9">
      <% if @model == 'user' %>
        <h3>ユーザー検索 '<%= @content %>'</h3>
          <%= render 'user/users/index', users: @detas %>
      <% else @model == 'menu' %>
        <h3>メニュー検索 '<%= @content %>'</h3>
          <%= render 'user/menus/index', menus: @detas %>
      <% end %>
      </div>
    </div>
</div>