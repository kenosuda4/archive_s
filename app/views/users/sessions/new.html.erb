<div class="container">
<div class="row no1">
<div class="col-xs-offset-3 col-xs-6">

<h2>会員の方はこちらからログイン</h2>

  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <table>
      <tr>
        <th>メールアドレス</th>
        <td><%= f.email_field :email, autofocus: true, autocomplete: "email", class: "col-xs-8" %></td>
      </tr>
      <tr>
        <th>パスワード</th>
        <td><%= f.password_field :password, autocomplete: "current-password", class: "col-xs-8" %></td>
      </tr>
      <tr>
        <th></th>
        <td></td>
      </tr>
    </table>
    <%= f.submit "ログイン", class: "btn btn-default" %>
  <% end %>
</div>  
<div class="col-xs-3">


<!-- <%= render "users/shared/links" %> -->

</div>
</div>
</div>