<h3>Users</h3>
<table class="table table-striped">
  <thead>
    <tr>
      <th>user name</th>
    </tr>
  </thead>
  <tbody>

  <% users.each do |user| %>
    <tr>
      <td>
        <%= link_to user_path(user.id) do %>
            <%= user.name %>
        <% end %>
     </tr>
  <% end %>
  </tbody>
 </table>